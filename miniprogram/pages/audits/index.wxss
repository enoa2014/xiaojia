.page { padding: 16rpx; }
.filters { display:flex; gap:12rpx; margin-bottom: 12rpx; }
.date { height: 72rpx; line-height:72rpx; padding: 0 16rpx; border:2rpx solid #D1D5DB; border-radius: 12rpx; min-width: 220rpx; color:#374151; }
.input { flex:1; height: 72rpx; padding: 0 16rpx; border:2rpx solid #D1D5DB; border-radius: 12rpx; }
.btn { height: 72rpx; padding: 0 24rpx; background:#22C55E; color:#fff; border-radius: 12rpx; }
.card { background:#fff; border-radius: 12rpx; box-shadow: 0 2rpx 8rpx rgba(0,0,0,0.04); padding: 16rpx; margin-bottom: 12rpx; }
.row { display:flex; justify-content:space-between; padding: 8rpx 0; border-bottom: 2rpx solid #F3F4F6; }
.row:last-child { border-bottom: none; }
.k { color:#6B7280; font-size: 18rpx; }
.v { color:#111827; font-size: 20rpx; word-break: break-all; }
.more { text-align:center; color:#1D4ED8; padding: 16rpx 0; }
.empty { text-align:center; color:#9CA3AF; padding: 40rpx 0; }
.error { color:#B91C1C; padding: 8rpx 0; }

