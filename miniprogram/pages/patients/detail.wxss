.page { padding: 16rpx; }
.card { background:#FFF; border-radius: 12rpx; box-shadow: 0 2rpx 8rpx rgba(0,0,0,0.04); padding: 16rpx; }
.row { display:flex; justify-content:space-between; padding: 10rpx 0; border-bottom: 2rpx solid #F3F4F6; }
.row:last-child { border-bottom: none; }
.k { color:#6B7280; font-size: 18rpx; }
.v { color:#111827; font-size: 20rpx; }
.empty { text-align:center; color:#9CA3AF; padding: 40rpx 0; }
.perm { margin-top: 12rpx; display:flex; align-items:center; gap: 12rpx; color:#374151; }
